<% provide(:title, "Log in") %>
<h1>Log in</h1>

	<div class="row">
		<div class="col-md-t col-md-offset-3">

			<%= f.label :email %>
			<%= f.email_field :email, class: 'form-control' %>

			<%= f.label :password_digest %>
			<%= f.password_field :password_digest, class: 'form-control' %>

			<%= f.label :first_name %>
			<%= f.text_field :first_name %>

			<%= f.label :last_name %>
			<%= f.text_field :last_name %>

			<%= f.label :created_at %>
			<%= f.datetime :created_at %>

			<%= f.label :updated_at %>
			<%= f.datetime :updated_at %>

			<%= f.sumbit "Log in", class: "btn btn-primary" %>

		<p>New user? <%= link_to "Sign up now!", signup_path %></p>
		</div>
	</div>
<% end %>

