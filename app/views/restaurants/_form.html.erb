<%= form_for(@restaurant) do |f| %>

	<div class="field">
		<%= f.label :email %>
		<%= f.text_field :email %>
	</div>
	<div class="field">
		<%= f.label :password_digest %>
		<%= f.text_field :password_digest %>
	</div>
	<div class="field">
		<%= f.label :first_name %>
		<%= f.text_field :first_name %>
	</div>
	<div class="field">
		<%= f.label :last_name %>
		<%= f.text_field :last_name %>
	</div>
	<div class="field>">
		<%= f.label :created_at %>
		<%= f.datetime_select :create_table %>
	</div>
	<div class="field>">
		<%= f.label :updated_at %>
		<%= f.datetime_select :updated_at %>
	</div>
<% end %>
